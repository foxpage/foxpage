{
  "name": "@foxpage/foxpage-admin",
  "version": "0.3.3",
  "description": "foxpage admin",
  "keywords": [
    "foxpage admin",
    "foxpage portal"
  ],
  "scripts": {
    "start": "webpack serve --config ./build/webpack.dev.config.js --progress --inline --color",
    "build": "npm run build:lib & npm run build:dist",
    "build:lib": "tsc --project tsconfig.json",
    "build:dist": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.prd.config.js",
    "clean": "rimraf dist",
    "test": "jest --report=jest-html-reporter --coverage --forceExit",
    "test:update": "npm run test -- --updateSnapshot",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --coverage --json --outputFile=./coverage/coverage-summary.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
